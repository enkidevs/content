{
  "name": "enki-content",
  "version": "0.5.3",
  "description": "parser of enki insights",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore __tests__",
    "test": "npm run lint && jest --notify",
    "test:watch": "jest --watchAll",
    "prepublish": "npm run test && npm run clean && npm run build"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enkidevs/content.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enkidevs/content/issues"
  },
  "homepage": "https://github.com/enkidevs/content#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "jest": "^21.2.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "js-yaml": "^3.6.1",
    "lodash.isequal": "^4.5.0"
  },
  "jest": {
    "testRegex": "/src/__tests__/.*\\.test.js$"
  }
}
